{
    "extends": "@apify/tsconfig",
    "compilerOptions": {
        "module": "ESNext",
        "target": "ESNext",
        "outDir": "dist",
        "noUnusedLocals": false,
        "lib": ["ES2022"],
        "skipLibCheck": true,
        "typeRoots": ["./types", "./node_modules/@types"],
        "baseUrl": ".",
        "paths": {
            "*": ["node_modules/*"]
        }
    },
    "include": [
        "./src/**/*", "./types/**/*"
    ]
}
